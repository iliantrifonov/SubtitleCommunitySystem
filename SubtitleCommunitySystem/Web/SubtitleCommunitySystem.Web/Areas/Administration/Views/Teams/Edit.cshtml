@model SubtitleCommunitySystem.Web.Areas.Administration.Models.CreateTeamViewModel

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>Team</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.Team.Id)

        @Html.DisplayForModel("CreateTeamViewModel")



        @{ Html.RenderPartial("_EditButtonsPartial"); }
    </div>
}

@section messages {
    @if (TempData["Success"] != null)
    {
        @Html.DisplayFor(c => TempData["Success"], "SuccessMessage")
    }
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
