@model IEnumerable<SubtitleCommunitySystem.Web.Areas.Administration.Models.UserOutputModel>

@{
    ViewBag.Title = "Index";
}

<table class="table">
    <tr>        
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @{ Html.RenderPartial("_CrudButtonsPartial", item); }
        </td>
    </tr>
}

</table>
