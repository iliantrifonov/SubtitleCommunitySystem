@model SubtitleCommunitySystem.Web.Areas.Private.Models.AddLanguageViewModel

<div class="form-group">
    @Html.LabelFor(model => model.LanguageId, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.DropDownListFor(m => m.LanguageId, Model.Languages, new { @class = "form-control" })
    </div>
</div>
