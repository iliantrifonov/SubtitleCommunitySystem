@model SubtitleCommunitySystem.Web.Areas.Private.Models.AddLanguageViewModel

@{
    ViewBag.Title = "Add";
}

<h2>Add Language</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>Choose Language</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        @Html.DisplayForModel("AddLanguageViewModel", Model)

        @{ Html.RenderPartial("_CreateButtonsPartial"); }
    </div>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}